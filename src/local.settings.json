{
  "IsEncrypted": false,
  "Values": {
    "AzureWebJobsStorage": "UseDevelopmentStorage=true",
    "FUNCTIONS_WORKER_RUNTIME": "python",

    "ENABLE_MCP_TRIGGERS": "true",

    "DEFAULT_ENGINES": "duckduckgo,qwant,google,wikipedia,wikidata,github,stackoverflow,hackernews",
    "DISABLE_ENGINES": "bing,bing images,bing news,bing videos",

    "REQUEST_TIMEOUT": "8.0",
    "MAX_REQUEST_TIMEOUT": "15.0",

    "SEARX_OUTGOING_TIMEOUT": "12",
    "SEARX_FORCE_IPV4": "true",
    "SEARX_HTTP_MAX_CONNECTIONS": "100",
    "SEARX_HTTP_MAX_KEEPALIVE": "50",
    "SEARX_REQUEST_JITTER_MS": "0"
  }
}
