{
    "mcpServers": {
        "remote-mcp-function": {
            "type": "sse",
            "url": "https://<functionapp-name>.azurewebsites.net/runtime/webhooks/mcp/sse",
            "headers": {
                "x-functions-key": "<your-mcp-extension-system-key>"
            }
        },
        "local-mcp-function": {
            "type": "sse",
            "url": "http://localhost:7071/runtime/webhooks/mcp/sse"
        }
    }
}